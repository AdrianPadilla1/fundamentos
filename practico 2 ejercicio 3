#include <iostream>
#include <cmath>

using namespace std;

int main()
{
    int i,N;
    float acum=0;
    do {
        cout<<"Ingrese el limite N: "<<endl;
        cin>>N;
    } while (N<0) ;
    for (i=1;i<=N;i++) {
        acum=acum+pow(i,2) ;
    }
    cout<<"El resultado es: "<<acum<<endl;

    return 0;
}
