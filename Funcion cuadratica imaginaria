#include <iostream>
#include <cmath>

using namespace std;

int main()
{
    float a,b,c,x1,x2;
    cout<<"FORMULA CUADRATICA"<<endl;
    
    cout<<"Ingresar el valor de a: "<<endl;
    cin>>a;
    cout<<"Ingresar el valor de b: "<<endl;
    cin>>b;
    cout<<"Ingrese el valor de c: "<<endl;
    cin>>c;
    x1=(-b+sqrt(b*b-4*a*c))/(2*a);
    x2=(-b-sqrt(b*b-4*a*c))/(2*a);
    cout<<"Los valores son: "<<x2<<endl;
    cout<<"Los valores son: "<<x1<<endl;
    
    return 0;
}
